@article{Pinheiro_2024,
  title     = "{Aves da {Reserva Biol{\'o}gica do Gurupi}: implementa{\c c}{\~a}o
               de protocolo avan{\c c}ado de monitoramento e atualiza{\c
               c}{\~o}es}",
  author    = "Pinheiro, L. V. S. and Leite, G. A. and
               Martinez, C. and Gonsioroski, G. and Pereira, S.
               and Silva de Melo, H. R. and Arantes, F. and Vieira
               Cerqueira, P. and Melinski, R. D. and Oliveira,
               A. and Abreu Bovo, A. A. and Guedes Las-Casas, F.
               M. and Guimar{\~a}es Carvalho, L. and 
               Sousa, A. E. B. A. and Ubaid, F. K.",
  abstract  = "O protocolo avan{\c c}ado de monitoramento de comunidades de
               aves, que faz parte do Programa Nacional de Monitoramento da
               Biodiversidade -- Programa Monitora, teve in{\'\i}cio em 2017 na
               Reserva Biol{\'o}gica do Gurupi (REBIO do Gurupi), escolhida
               devido ao contexto de degrada{\c c}{\~a}o em que est{\'a}
               inserida. Os resultados aqui apresentados foram obtidos ao longo
               de seis campanhas realizadas entre 2017--2023 na aplica{\c
               c}{\~a}o do protocolo e oportunisticamente. Foram registradas
               414 esp{\'e}cies, sendo Cercomacra cinerascens a mais abundante.
               O monitoramento contribuiu com tr{\^e}s esp{\'e}cies novas para
               o estado do Maranh{\~a}o, Agamia agami (primeiro registro
               documentado para o estado), Forpus passerinus e Leucopternis
               melanops. Quando compilados os dados com o primeiro
               invent{\'a}rio de aves da REBIO do Gurupi, a riqueza aumentou
               para 503 esp{\'e}cies, passando a ser assim a {\'a}rea com a
               maior riqueza de aves dentro da {\'a}rea de endemismo de
               Bel{\'e}m (AEB). Desse total, 29 taxa est{\~a}o amea{\c c}ados
               de extin{\c c}{\~a}o (e.g. Crax fasciolata pinima e Psophia
               obscura) e 18 s{\~a}o end{\^e}micas da AEB. Esses resultados
               tamb{\'e}m contribu{\'\i}ram para atualiza{\c c}{\~a}o do
               cen{\'a}rio ornitol{\'o}gico da Amaz{\^o}nia Maranhense, que
               agora conta com 583 esp{\'e}cies. Dessa forma, as informa{\c
               c}{\~o}es aqui geradas indicam a relev{\^a}ncia e efetividade da
               REBIO do Gurupi para a conserva{\c c}{\~a}o de aves no
               Maranh{\~a}o e na AEB, e a import{\^a}ncia do Programa Monitora,
               sendo assim recomendada sua continuidade e expans{\~a}o.",
  journal   = "Biodivers. Bras.",
  publisher = "Instituto Chico Mendes de Conservacao da Biodiversidade - ICBBio",
  volume    =  14,
  number    =  3,
  pages     = "71--104",
  month     =  aug,
  year      =  2024
}





